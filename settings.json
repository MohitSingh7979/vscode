{
	"git.autofetch": true,
	"editor.fontFamily": "Fira Code Regular",
	"better-comments.tags": [
		{
			"tag": "!",
			"color": "#FF2D00",
			"strikethrough": false,
			"underline": false,
			"backgroundColor": "transparent",
			"bold": false,
			"italic": false
		},
		{
			"tag": "?",
			"color": "#3498DB",
			"strikethrough": false,
			"underline": false,
			"backgroundColor": "transparent",
			"bold": false,
			"italic": false
		},
		{
			"tag": "//",
			"color": "#00ff00",
			"strikethrough": true,
			"underline": false,
			"backgroundColor": "transparent",
			"bold": false,
			"italic": false
		},
		{
			"tag": "todo",
			"color": "#FF8C00",
			"strikethrough": false,
			"underline": false,
			"backgroundColor": "transparent",
			"bold": false,
			"italic": false
		},
		{
			"tag": "*",
			"color": "#98C379",
			"strikethrough": false,
			"underline": false,
			"backgroundColor": "transparent",
			"bold": false,
			"italic": false
		},
		{
			"tag": "#",
			"color": "#aaaaaa",
			"strikethrough": false,
			"underline": false,
			"backgroundColor": "transparent",
			"bold": false,
			"italic": false
		}
	],
	"remoteHub.commitDirectlyWarning": "off",
	"security.workspace.trust.untrustedFiles": "open",
	"git.openRepositoryInParentFolders": "always",
	"[python]": {
		"editor.formatOnType": true,
		"editor.defaultFormatter": "ms-python.autopep8"
	},
	"workbench.editorAssociations": {
		"*.py": "default",
		"*.md": "default",
		"*.jpg": "cweijan.officeViewer"
	},
	"jupyter.askForKernelRestart": false,
	"editor.tabSize": 2,
	"editor.codeActionsOnSave": {},
	"explorer.sortOrder": "type",
	"explorer.confirmDelete": false,
	"html.format.maxPreserveNewLines": 1,
	"css.format.maxPreserveNewLines": 0,
	"editor.minimap.enabled": false,
	"liveServer.settings.donotShowInfoMsg": true,
	"liveServer.settings.donotVerifyTags": true,
	"scm.defaultViewMode": "tree",
	"search.defaultViewMode": "tree",
	"hediet.vscode-drawio.resizeImages": null,
	"terminal.integrated.gpuAcceleration": "on",
	"terminal.integrated.letterSpacing": 1,
	"diffEditor.ignoreTrimWhitespace": false,
	"remote.portsAttributes": {
		"443": {
			"protocol": "https"
		},
		"8443": {
			"protocol": "https"
		}
	},
	"liveServer.settings.host": "localhost",
	"git.confirmSync": false,
	"terminal.integrated.defaultProfile.windows": "Git Bash",
	"terminal.integrated.profiles.windows": {
		"PowerShell": {
			"source": "PowerShell",
			"icon": "terminal-powershell"
		},
		"Command Prompt": {
			"path": [
				"${env:windir}\\Sysnative\\cmd.exe",
				"${env:windir}\\System32\\cmd.exe"
			],
			"args": [],
			"icon": "terminal-cmd"
		},
		"Git Bash": {
			"source": "Git Bash"
		},
		"Ubuntu (WSL)": {
			"path": "C:\\WINDOWS\\System32\\wsl.exe",
			"args": ["-d", "Ubuntu"]
		}
	},
	"editor.linkedEditing": true,
	"editor.fontSize": 18,
	"editor.fontLigatures": true,
	"terminal.integrated.fontFamily": "Fira Code Regular",
	"terminal.integrated.lineHeight": 1.1,
	"path-intellisense.autoTriggerNextSuggestion": true,
	"emmet.triggerExpansionOnTab": true,
	"editor.guides.bracketPairs": true,
	"json.format.enable": false,
	"typescript.format.enable": false,
	"typescript.format.indentSwitchCase": false,
	"css.format.newlineBetweenRules": false,
	"css.format.newlineBetweenSelectors": false,
	"css.format.preserveNewLines": false,
	"less.format.enable": false,
	"less.format.newlineBetweenRules": false,
	"less.format.newlineBetweenSelectors": false,
	"less.format.preserveNewLines": false,
	"scss.format.enable": false,
	"scss.format.newlineBetweenRules": false,
	"scss.format.newlineBetweenSelectors": false,
	"scss.format.preserveNewLines": false,
	"eslint.format.enable": true,
	"workbench.iconTheme": "material-icon-theme",
	"files.insertFinalNewline": true,
	"editor.rename.enablePreview": false,
	"notebook.lineNumbers": "on",
	"markdown.validate.unusedLinkDefinitions.enabled": "warning",
	"notebook.output.fontFamily": "Fira Code",
	"notebook.output.wordWrap": true,
	"javascript.format.insertSpaceAfterConstructor": true,
	"javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,
	"javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyParenthesis": true,
	"javascript.format.insertSpaceBeforeFunctionParenthesis": true,
	"javascript.format.semicolons": "insert",
	"javascript.inlayHints.parameterNames.enabled": "all",
	"javascript.suggest.completeFunctionCalls": true,
	"typescript.suggest.completeFunctionCalls": true,
	"markdown.preview.fontSize": 18,
	"explorer.openEditors.minVisible": 1,
	"explorer.openEditors.sortOrder": "fullPath",
	"workbench.tree.indent": 15,
	"workbench.tree.renderIndentGuides": "always",
	"typescript.inlayHints.functionLikeReturnTypes.enabled": true,
	"typescript.referencesCodeLens.enabled": true,
	"files.trimTrailingWhitespace": true,
	"terminal.integrated.profiles.linux": {
		"bash": {
			"path": "bash",
			"icon": "terminal-bash"
		},
		"zsh": {
			"path": "zsh"
		},
		"fish": {
			"path": "fish"
		},
		"tmux": {
			"path": "tmux",
			"icon": "terminal-tmux"
		},
		"pwsh": {
			"path": "pwsh",
			"icon": "terminal-powershell"
		}
	},
	"terminal.integrated.useWslProfiles": false,
	"typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,
	"editor.autoClosingBrackets": "always",
	"liveServer.settings.fullReload": true,
	"terminal.integrated.fontSize": 17,
	"gitlens.views.contributors.showStatistics": true,
	"gitHistory.sideBySide": true,
	"typescript.format.semicolons": "insert",
	"javascript.format.insertSpaceAfterOpeningAndBeforeClosingTemplateStringBraces": true,
	"typescript.format.insertSpaceAfterConstructor": true,
	"typescript.format.insertSpaceAfterOpeningAndBeforeClosingJsxExpressionBraces": true,
	"typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyParenthesis": true,
	"typescript.format.insertSpaceAfterOpeningAndBeforeClosingTemplateStringBraces": true,
	"typescript.format.insertSpaceAfterTypeAssertion": true,
	"typescript.format.insertSpaceBeforeFunctionParenthesis": true,
	"editor.formatOnPaste": true,
	"prettier.proseWrap": "always",
	"editor.wrappingIndent": "indent",
	"editor.formatOnSave": true,
	"editor.wordWrapColumn": 60,
	"html.format.indentHandlebars": true,
	"html.format.indentInnerHtml": true,
	"html.format.templating": true,
	"[html]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[javascript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"prettier.bracketSameLine": true,
	"git.enableSmartCommit": true,
	"editor.inlayHints.padding": true,
	"javascript.inlayHints.enumMemberValues.enabled": true,
	"javascript.inlayHints.functionLikeReturnTypes.enabled": true,
	"typescript.inlayHints.enumMemberValues.enabled": true,
	"typescript.inlayHints.parameterTypes.enabled": true,
	"typescript.inlayHints.propertyDeclarationTypes.enabled": true,
	"typescript.inlayHints.variableTypes.enabled": true,
	"typescript.updateImportsOnFileMove.enabled": "always",
	"editor.glyphMargin": false,
	"emmet.showSuggestionsAsSnippets": true,
	"emmet.useInlineCompletions": true,
	"python.analysis.inlayHints.callArgumentNames": "all",
	"python.analysis.inlayHints.functionReturnTypes": true,
	"python.analysis.inlayHints.pytestParameters": true,
	"python.analysis.inlayHints.variableTypes": true,
	"python.analysis.autoImportCompletions": false,
	"python.analysis.completeFunctionParens": true,
	"editor.suggest.matchOnWordStartOnly": true,
	"editor.inlineSuggest.showToolbar": "always",
	"editor.suggest.showStatusBar": true,
	"editor.lineDecorationsWidth": 0,
	"editor.showDeprecated": false,
	"editor.snippetSuggestions": "top",
	"editor.quickSuggestionsDelay": 1,
	"editor.inlineSuggest.suppressSuggestions": true,
	"editor.quickSuggestions": {
		"comments": "on",
		"strings": "on"
	},
	"editor.suggest.preview": true,
	"terminal.external.windowsExec": "C:\\Program Files\\Git\\git-bash.exe",
	"r.rterm.windows": "C:\\Program Files\\R\\R-4.3.1\\bin\\x64\\Rterm.exe",
	"editor.accessibilitySupport": "off",
	"workbench.activityBar.location": "top",
	"files.autoSave": "afterDelay",
	"liveServer.settings.CustomBrowser": "chrome:PrivateMode",
	"prettier.quoteProps": "consistent",
	"search.smartCase": true,
	"prettier.useTabs": true,
	"editor.wordBasedSuggestions": "allDocuments",
	"window.commandCenter": false,
	"workbench.layoutControl.enabled": false,
	"search.showLineNumbers": true,
	"editor.suggestSelection": "recentlyUsedByPrefix",
	"javascript.inlayHints.parameterTypes.enabled": true,
	"audioCues.noInlayHints": "on",
	"files.trimFinalNewlines": true,
	"css.format.spaceAroundSelectorSeparator": true,
	"breadcrumbs.enabled": false,
	"window.zoomLevel": 1,
	"[markdown]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"javascript.referencesCodeLens.showOnAllFunctions": true,
	"typescript.referencesCodeLens.showOnAllFunctions": true,
	"workbench.settings.applyToAllProfiles": [
		"editor.formatOnType",
		"editor.defaultFormatter"
	],
	"editor.formatOnType": true,
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"files.autoSaveDelay": 400,
	"editor.suggestOnTriggerCharacters": true,
	"typescript.inlayHints.parameterNames.enabled": "all",
	"html-css-class-completion.enableEmmetSupport": true,
	"tailwindCSS.emmetCompletions": true,
	"css.autoValidation": "Always",
	"css.styleSheets": [
		"node_modules/bootstrap/dist/css/bootstrap.css",
		"src/**/*.scss",
		"https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css",
		"src/**/*.css"
	],
	"css.enabledLanguages": ["css", "html"],
	"editor.suggest.showProperties": true,
	"editor.inlineSuggest.enabled": true,
	"editor.suggest.showWords": true
}
