{
  "git.autofetch": true,
  "editor.fontFamily": "Fira Code Regular",
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "editor.autoClosingBrackets": "always",
  "gitHistory.sideBySide": true,
  "git.enableSmartCommit": true,
  "editor.renderWhitespace": "all" /* show dots on spaces */,
  "editor.linkedEditing": true,
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.definitionLinkOpensInPeek": false,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "notebook.formatOnSave.enabled": true,
  "notebook.formatOnCellExecution": true,
  "liveServer.settings.donotShowInfoMsg": true /* popup */,
  "workbench.startupEditor": "none",
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "editor.wordWrap": "on",
  "editor.guides.bracketPairs": true,
  "css.format.spaceAroundSelectorSeparator": true,
  "editor.experimentalInlineEdit.backgroundColoring": true,
  "editor.defaultColorDecorators": true /* extension requirement */,
  "html.format.templating": true,
  "html.format.indentHandlebars": true /* file icon required extension */,
  "css.format.preserveNewLines": false,
  "explorer.confirmDelete": false,
  "workbench.editor.decorations.colors": true,
  "C_Cpp.enhancedColorization": "enabled" /* enabled */,
  "C_Cpp.clang_format_style": "file",
  "C_Cpp.formatting": "vcFormat" /* must be vcFormat: bcoz of {} */,
  "problems.showCurrentInStatus": true,
  // "indentRainbow.excludedLanguages": [],
  "indentRainbow.updateDelay": 1,
  // "indentRainbow.colorOnWhiteSpaceOnly": true, /* true */
  "better-comments.tags": [
    /* extension required */
    {
      "tag": ".",
      "color": "#FFF",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "!",
      "color": "#FF2D00",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "?",
      "color": "#3498DB",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "//",
      "color": "#fff" /* #00ff00 */,
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "todo",
      "color": "#FF8C00",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "*",
      "color": "#98C379",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "#",
      "color": "#aaaaaa",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    }
  ],
  "better-comments.multilineComments": true,
  "workbench.preferredHighContrastColorTheme": "Monokai" /* Monokai or Dracula */,
  // "C_Cpp.vcFormat.newLine.closeBraceSameLine.emptyFunction": true,
  // "C_Cpp.vcFormat.newLine.closeBraceSameLine.emptyType": true,
  // "C_Cpp.vcFormat.newLine.scopeBracesOnSeparateLines": true,
  "gitHistory.alwaysPromptRepositoryPicker": true,
  "task.quickOpen.showAll": true,
  "editor.hover.delay": 0 /* 10 */,
  "editor.hover.hidingDelay": 0 /* 10 */,
  "errorLens.editorHoverPartsEnabled": {
    /* all true */
    "messageEnabled": true,
    "sourceCodeEnabled": true,
    "buttonsEnabled": true
  },
  "editor.experimentalInlineEdit.showToolbar": "always",
  "editor.inlineSuggest.showToolbar": "always",
  "workbench.tree.renderIndentGuides": "always",
  "workbench.view.alwaysShowHeaderActions": true,
  "[markdown]": {
    "editor.defaultFormatter": "DavidAnson.vscode-markdownlint",
    "editor.quickSuggestions": {
      "other": true,
      "comments": true,
      "strings": true
    }
  },
  "editor.minimap.enabled": false,
  "liveServer.settings.CustomBrowser": "chrome:PrivateMode" /* must be private : temp file info saved */,
  "markdown.extension.completion.enabled": true,
  "terminal.integrated.initialHint": true /* true */,
  "editor.quickSuggestions": {
    /* all true */
    "comments": "on",
    "strings": "on"
  },
  "editor.quickSuggestionsDelay": 0 /* true */,
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "colorful-comments.tags": [
    /* best extension for comments */
    {
      "tag": "!",
      "color": "#FF2D00",
      "strikethrough": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "?",
      "color": "#0076FF",
      "strikethrough": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "-",
      "color": "#fff",
      /* #474747 */ "strikethrough": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "^",
      "color": "#EAF622",
      "strikethrough": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "*",
      "color": "#28FF00",
      "strikethrough": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "&",
      "color": "#FF06A0",
      "strikethrough": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "~",
      "color": "#BE00FF",
      "strikethrough": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "todo",
      "color": "#FF8C00",
      "strikethrough": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    }
  ],
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "html.format.maxPreserveNewLines": 1 /* 1 */,
  "editor.tokenColorCustomizations": {
    /* inbuild customize coloring */
    "comments": {
      "foreground": "#000" /* "#f88" */,
      "fontStyle": "italic"
    }
  },
  "explorer.compactFolders": false /* false: hard to understand file tree */,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.fontLigatures": true /* for fira code fonts */,
  "workbench.activityBar.location": "top",
  /* above code works fine */
  "editor.showFoldingControls": "always",
  "editor.semanticHighlighting.enabled": true /* must be true */,
  "[cpp]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  // "diffEditor.ignoreTrimWhitespace": true,
  "better-comments.highlightPlainText": true /* true */,
  "colorful-comments.highlightPlainText": true,
  /* belows settings are for experiments only */
