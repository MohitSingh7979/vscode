{
  "git.autofetch": true,
  "editor.fontFamily": "Fira Code Regular",
  "better-comments.tags": [
    {
      "tag": "!",
      "color": "#FF2D00",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "?",
      "color": "#3498DB",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "//",
      "color": "#00ff00",
      "strikethrough": true,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "todo",
      "color": "#FF8C00",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "*",
      "color": "#98C379",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "#",
      "color": "#aaaaaa",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    }
  ],
  "remoteHub.commitDirectlyWarning": "off",
  "security.workspace.trust.untrustedFiles": "open",
  "git.openRepositoryInParentFolders": "always",
  "[python]": {
    "editor.formatOnType": true,
    "editor.defaultFormatter": "ms-python.autopep8"
  },
  "workbench.editorAssociations": {
    "*.py": "default"
  },
  "jupyter.askForKernelRestart": false,
  "editor.tabSize": 2,
  "editor.codeActionsOnSave": {},
  "explorer.sortOrder": "type",
  "explorer.confirmDelete": false,
  "html.format.maxPreserveNewLines": 1,
  "css.format.maxPreserveNewLines": 1,
  "editor.minimap.enabled": false,
  "liveServer.settings.donotShowInfoMsg": true,
  "liveServer.settings.donotVerifyTags": true,
  "scm.defaultViewMode": "tree",
  "search.defaultViewMode": "tree",
  "hediet.vscode-drawio.resizeImages": null,
  "terminal.integrated.gpuAcceleration": "on",
  "terminal.integrated.letterSpacing": 1,
  "diffEditor.ignoreTrimWhitespace": false,
  "remote.portsAttributes": {
    "443": {
      "protocol": "https"
    },
    "8443": {
      "protocol": "https"
    }
  },
  "liveServer.settings.host": "localhost",
  "git.confirmSync": false,
  "files.autoSave": "afterDelay",
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "terminal.integrated.profiles.windows": {
    "PowerShell": {
      "source": "PowerShell",
      "icon": "terminal-powershell"
    },
    "Command Prompt": {
      "path": [
        "${env:windir}\\Sysnative\\cmd.exe",
        "${env:windir}\\System32\\cmd.exe"
      ],
      "args": [],
      "icon": "terminal-cmd"
    },
    "Git Bash": {
      "source": "Git Bash"
    },
    "Ubuntu (WSL)": {
      "path": "C:\\WINDOWS\\System32\\wsl.exe",
      "args": ["-d", "Ubuntu"]
    }
  },
  "editor.linkedEditing": true,
  "editor.fontSize": 18,
  "editor.fontLigatures": true,
  "terminal.integrated.fontFamily": "Fira Code Regular",
  "terminal.integrated.lineHeight": 1.1,
  "path-intellisense.autoTriggerNextSuggestion": true,
  "emmet.triggerExpansionOnTab": true,
  "editor.guides.bracketPairs": true,
  "json.format.enable": false,
  "typescript.format.enable": false,
  "typescript.format.indentSwitchCase": false,
  "css.format.enable": false,
  "css.format.newlineBetweenRules": false,
  "css.format.newlineBetweenSelectors": false,
  "css.format.preserveNewLines": false,
  "less.format.enable": false,
  "less.format.newlineBetweenRules": false,
  "less.format.newlineBetweenSelectors": false,
  "less.format.preserveNewLines": false,
  "scss.format.enable": false,
  "scss.format.newlineBetweenRules": false,
  "scss.format.newlineBetweenSelectors": false,
  "scss.format.preserveNewLines": false,
  "eslint.format.enable": true,
  "workbench.colorTheme": "Monokai",
  "workbench.iconTheme": "material-icon-theme",
  "files.insertFinalNewline": true,
  "editor.rename.enablePreview": false,
  "notebook.lineNumbers": "on",
  "markdown.validate.unusedLinkDefinitions.enabled": "warning",
  "notebook.output.fontFamily": "Fira Code",
  "notebook.output.wordWrap": true,
  "editor.find.autoFindInSelection": "always",
  "javascript.format.insertSpaceAfterConstructor": true,
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyParenthesis": true,
  "javascript.format.insertSpaceBeforeFunctionParenthesis": true,
  "javascript.format.semicolons": "insert",
  "javascript.inlayHints.parameterNames.enabled": "all",
  "javascript.suggest.completeFunctionCalls": true,
  "typescript.suggest.completeFunctionCalls": true,
  "markdown.preview.fontSize": 18,
  "explorer.openEditors.minVisible": 1,
  "explorer.openEditors.sortOrder": "fullPath",
  "workbench.tree.indent": 15,
  "workbench.tree.renderIndentGuides": "always",
  "typescript.inlayHints.functionLikeReturnTypes.enabled": true,
  "typescript.referencesCodeLens.enabled": true,
  "files.trimTrailingWhitespace": true,
  "terminal.integrated.profiles.linux": {
    "bash": {
      "path": "bash",
      "icon": "terminal-bash"
    },
    "zsh": {
      "path": "zsh"
    },
    "fish": {
      "path": "fish"
    },
    "tmux": {
      "path": "tmux",
      "icon": "terminal-tmux"
    },
    "pwsh": {
      "path": "pwsh",
      "icon": "terminal-powershell"
    }
  },
  "terminal.integrated.useWslProfiles": false,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,
  "editor.autoClosingBrackets": "always",
  "liveServer.settings.fullReload": true,
  "liveServer.settings.NoBrowser": true,
  "javascript.inlayHints.parameterNames.suppressWhenArgumentMatchesName": false,
  "typescript.inlayHints.parameterNames.suppressWhenArgumentMatchesName": false,
  "terminal.integrated.fontSize": 17,
  "gitlens.views.contributors.showStatistics": true,
  "gitHistory.sideBySide": true,
  "typescript.format.semicolons": "insert",
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingTemplateStringBraces": true,
  "typescript.format.insertSpaceAfterConstructor": true,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingJsxExpressionBraces": true,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyParenthesis": true,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingTemplateStringBraces": true,
  "typescript.format.insertSpaceAfterTypeAssertion": true,
  "typescript.format.insertSpaceBeforeFunctionParenthesis": true,
  "html.format.wrapAttributes": "force-aligned",
  "editor.formatOnType": true,
  "editor.formatOnPaste": true,
  "prettier.proseWrap": "always",
  "editor.wrappingIndent": "indent",
  "editor.formatOnSave": true,
  "editor.wordWrapColumn": 60,
  "editor.wordWrap": "on",
  "html.format.indentHandlebars": true,
  "html.format.indentInnerHtml": true,
  "html.format.templating": true,
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "prettier.bracketSameLine": true,
  "git.enableSmartCommit": true,
  "window.zoomLevel": 1,
  "editor.inlayHints.padding": true,
  "javascript.inlayHints.enumMemberValues.enabled": true,
  "javascript.inlayHints.functionLikeReturnTypes.enabled": true,
  "typescript.inlayHints.enumMemberValues.enabled": true,
  "typescript.inlayHints.parameterTypes.enabled": true,
  "typescript.inlayHints.propertyDeclarationTypes.enabled": true,
  "typescript.inlayHints.variableTypes.enabled": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "editor.folding": false,
  "editor.glyphMargin": false,
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.useInlineCompletions": true,
  "python.analysis.inlayHints.callArgumentNames": true,
  "python.analysis.inlayHints.functionReturnTypes": true,
  "python.analysis.inlayHints.pytestParameters": true,
  "python.analysis.inlayHints.variableTypes": true,
  "python.analysis.autoImportCompletions": false,
  "python.analysis.completeFunctionParens": true,
  "editor.suggest.matchOnWordStartOnly": true,
  "editor.inlineSuggest.showToolbar": "always",
  "editor.suggest.showStatusBar": true,
  "editor.lineDecorationsWidth": 0,
  "workbench.activityBar.visible": false,
  "editor.inlineSuggest.suppressSuggestions": true,
  "editor.suggest.showKeywords": false,
  "editor.showDeprecated": false,
  "editor.suggest.showDeprecated": false,
  "editor.suggest.showMethods": false,
  "editor.snippetSuggestions": "top",
  "editor.quickSuggestions": {
    "comments": "on",
    "strings": "on"
  },
  "editor.quickSuggestionsDelay": 5,
  "editor.suggest.showProperties": false,
  "editor.suggest.showTypeParameters": false,
  "editor.suggest.showWords": false
}
