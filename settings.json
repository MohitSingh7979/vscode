  // "C_Cpp.vcFormat.space.beforeBlockOpenBrace": false,
  // "C_Cpp.vcFormat.space.preserveInInitializerList": false,
  "colorful-comments.multilineComments": true,
  "git.confirmSync": false,
  "editor.unfoldOnClickAfterEndOfLine": true,
  "workbench.colorTheme": "Default Light Modern",
  "liveServer.settings.donotVerifyTags": true,
  "html.format.extraLiners": "body,/html",
  "[java]": {
    "editor.defaultFormatter": "Oracle.oracle-java"
  },
  "editor.tabCompletion": "on",
  "editor.suggest.showInlineDetails": true,
  "editor.suggest.preview": true,

  "editor.lineDecorationsWidth": 0,
  "editor.glyphMargin": false,
  "editor.folding": true,
  "code-runner.saveFileBeforeRun": true,
  "code-runner.showExecutionMessage": false,
  "code-runner.clearPreviousOutput": true,
  "editor.tabFocusMode": false,
  "editor.codeLens": true,
  "editor.inlayHints.enabled": "on",
  "editor.inlineSuggest.enabled": true,
  "gitlens.graph.minimap.additionalTypes": [
    "localBranches",
    "stashes",
    "remoteBranches",
    "pullRequests",
    "tags"
  ],
  "javascript.suggest.completeFunctionCalls": true,
  "emmet.triggerExpansionOnTab": true,
  "files.associations": {
    "*.js": "javascriptreact"
  } /* true */,
  "editor.suggest.showProperties": true,
  "reactSnippets.settings.typescript": false,
  "reactSnippets.settings.languageScopes": "javascriptreact",

  /* gitlens*/
